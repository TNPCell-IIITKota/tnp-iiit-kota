{
  "private": true,
  "name": "tnp-iiit-kota",
  "version": "1.0.1",
  "description": "The Landing Page of Training and Placement Cell - IIIT Kota",
  "license": "MIT",
  "author": "Divyansh Singh <divyansh.singh@ikcb.org> (https://github.com/brc-dd)",
  "repository": "https://github.com/brc-dd/tnp-iiit-kota",
  "scripts": {
    "audit:fix": "bash -c \"npx npm-check-updates -u; npm i --package-lock-only; npm audit fix; rm yarn.lock; yarn import; rm package-lock.json\"",
    "build": "next build",
    "dev": "bash -c \"rm -r .next; yarn; next dev\"",
    "lint:code": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "lint:staged": "lint-staged --allow-empty --concurrent 1",
    "prepare": "husky install",
    "preprettify": "bash -c \"git ls-files '*.js' '*.ts' '*.tsx' '*.json' | xargs sed -i ':a;N;$!ba;s/{\\s*\\n/{ /g'\"",
    "prettify": "bash -c \"echo 'PASS 1'; prettier --write . --ignore-path ./.gitignore\"",
    "postprettify": "bash -c \"echo 'PASS 2'; prettier --write . --ignore-path ./.gitignore\"",
    "start": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "next": "canary",
    "next-transpile-modules": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recharts": "^2.1.4",
    "sharp": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "autoprefixer": "^10.3.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cssnano-preset-advanced": "^5.1.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "loader-utils": "^2.0.0",
    "postcss": "^8.3.8",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "sass": "^1.42.1",
    "tailwindcss": "2.2.4",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0"
  },
  "browserslist": [
    "> 0.3%",
    "not dead"
  ]
}
